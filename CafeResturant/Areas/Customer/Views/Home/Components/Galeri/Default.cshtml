<div class="wrap-gallery-footer flex-w">
	@foreach (var item in Model)
{

			<a class="item-gallery-footer wrap-pic-w" href="~/site/images/photo-gallery-01.jpg" data-lightbox="gallery-footer">
			<img src="@item.Image" alt="GALLERY" >
			</a>
		
}
</div>